set term postscript eps enhanced "Times" 25
set output 'wedge-residuals.eps'
set title 'Residual history: Mach 10 flow over 6 deg wedge'
set logscale y
set ylabel 'relative residuals, ||F(u^{max})||/||F(u^0)||'
set xlabel 'iteration count'
plot 'e4sss.diagnostics.dat' u 1:6 t 'mass' w linesp, \
     'e4sss.diagnostics.dat' u 1:8 t 'x-mom' w linesp, \
     'e4sss.diagnostics.dat' u 1:10 t 'y-mom' w linesp, \
     'e4sss.diagnostics.dat' u 1:12 t 'energy' w linesp 
