FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];  // Note: This are units for incompressible flow simulation. Here P_sim = P_actual / rho

internalField   uniform 0; // Can be kept at 0 for incompressible simulations. 

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }

    upperWall
    {
        type            zeroGradient;
    }

    lowerWall
    {
        type            zeroGradient;
    }
    symmetry
    {
        type            symmetry;
    }
    frontAndBack
    {
        type            empty;
    }
    wedge-top
    {
        type            wedge;
    }
    wedge-bottom
    {
        type            wedge;
    }
}


// ************************************************************************* //
