FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type                fixedValue;
        value               uniform 2e-6;
    }

    outlet
    {
        type                inletOutlet; // preferred setting for turbulence at outlet
        inletValue          uniform 0;
        value               uniform 0;
    }

    upperWall
    {
        type                fixedValue;
        value               uniform 0;
    }

    lowerWall
    {
        type                fixedValue;
        value               uniform 0;
    }
    symmetry
    {
        type            symmetry;
    }
    frontAndBack
    {
        type            empty;
    }
    wedge-top
    {
        type            wedge;
    }
    wedge-bottom
    {
        type            wedge;
    }
}

// ************************************************************************* //
